<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NameEmail">
        <attr name="android:background" />

        <attr name="name" format="string" />
        <attr name="nameTextColor" format="color" />

        <attr name="email" format="string" />
        <attr name="emailTextColor" format="color" />

        <attr name="layoutDirection" format="integer">
            <enum name="ltr" value="0" />
            <enum name="rtl" value="1" />
        </attr>
        <attr name="arrow" format="reference" />
    </declare-styleable>
</resources>