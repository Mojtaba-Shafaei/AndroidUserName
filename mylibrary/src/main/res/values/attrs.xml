<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="NameEmail">
    <attr name="android:background"/>

    <attr format="string" name="name"/>
    <attr format="color" name="nameTextColor"/>

    <attr format="string" name="email"/>
    <attr format="color" name="emailTextColor"/>

    <attr format="reference" name="arrow"/>
    <attr format="integer" name="name_layout_direction">
      <enum name="ltr" value="0"/>
      <enum name="rtl" value="1"/>
    </attr>
  </declare-styleable>
</resources>